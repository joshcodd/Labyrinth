<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbox" maxHeight="500.0" maxWidth="500.0" minHeight="400.0" minWidth="175.0" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="playerLabel" text="Player 1">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </Label>
      <ChoiceBox fx:id="profileBox" maxHeight="30.0" maxWidth="430.0" minHeight="30.0" minWidth="150.0" prefHeight="30.0" prefWidth="250.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ChoiceBox>
      <HBox maxHeight="50.0" maxWidth="450.0" minWidth="200.0" prefHeight="30.0" prefWidth="330.0">
         <children>
            <ChoiceBox fx:id="colourBox" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="150.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
            <Button fx:id="confButt" maxHeight="40.0" maxWidth="200.0" minHeight="20.0" minWidth="60.0" mnemonicParsing="false" prefHeight="30.0" text="Button">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <CheckBox fx:id="startFirst" mnemonicParsing="false" text="Starting first">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </CheckBox>
      <Label fx:id="confLabel" text="Label">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font size="18.0" />
         </font></Label>
      <StackPane fx:id="tankStack" maxHeight="302.0" maxWidth="500.0" minHeight="150.0" minWidth="150.0" prefHeight="302.0" prefWidth="349.0">
         <children>
            <ImageView fx:id="tankView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="colourLabel" text="Label" />
         </children>
      </StackPane>
      <Label fx:id="startLabel" text="Label">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
